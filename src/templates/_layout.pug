mjml
    //- Head
    mj-head
        mj-title Lancement du jeu concours 2025
        mj-font(name="Roboto" href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100..900;1,100..900&display=swap")/
        mj-font(name="Montserrat" href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap")/
        mj-raw.
            <meta charset="UTF-8">
            <meta name='color-scheme' content='light'/>
            <meta name='supported-color-schemes' content='light'/>
            <meta name="format-detection" content="telephone=no"/>
            <meta name="format-detection" content="date=no"/>
            <meta name="format-detection" content="address=no"/>
            <meta name="format-detection" content="email=no"/>
        mj-attributes
            mj-all(font-family="'Roboto', Arial, Helvetica, sans-serif")/
            mj-class(name='T36' font-style="normal" font-size="36px")/
            mj-class(name='T30' font-style="normal" font-size="30px")/
            mj-class(name='T22' font-style="normal" font-size="22px")/
            mj-class(name='T15' font-style="normal" font-size="15px")/
            mj-class(name='T14' font-style="normal" font-size="14px")/
            //- font-size / line-height
            mj-class(name='T60' font-style="normal" font-size='60px' line-height="68px")/
            mj-class(name='T36' font-style="normal" font-size='36px' line-height="46px")/
            mj-class(name='T18' font-style="normal" font-size='18px' line-height="24px")/
            mj-class(name='T16' font-style="normal" font-size='16px' line-height="24px")/
            mj-class(name='T12' font-style="normal" font-size='12px' line-height="18px")/
            //- font-weight
            mj-class(name="fw0" font-weight="400")/
            mj-class(name="fw1" font-weight="600")/
            mj-class(name="fw2" font-weight="700")/
            //-line heigth
            mj-class(name="lh" line-height="1.2")/
            //- color
            mj-class(name="cTW0" color="#ffffff")/
            mj-class(name="cTW+1" color="#EEF1F5")/
            mj-class(name="cTW+2" color="#DEE2E5")/
            mj-class(name="cTW+3" color="#666666")
            mj-class(name="cTB" color="#000000")
            mj-class(name="cbgContainer" container-background-color="#5A5656")/
            mj-class(name="bgFsombre" background-color="#212121")/
            mj-class(name="bgFclair" background-color="#eaeaea")/
            mj-class(name="bgV" background-color="#00412b")/
            mj-class(name="bgV+2" background-color="#002b1c")/
            //- compoments
            mj-divider(padding="0" border-style="solid" border-width="1px" border-color="#eeeeee")
            mj-button(padding="0" inner-padding="14px 25px" font-family="'Montserrat', Arial, Helvetica, sans-serif" font-size="16px" font-weight="700" line-height="18px" color="#ffffff" background-color="#9a7800" letter-spacing="3.5px" text-transform="uppercase" border-radius="0")
        mj-breakpoint(width="480px")
        mj-style.   
            @media only screen and (max-width:480px) {
            .desktop-only td {
                display: none !important;
                mso-hide: all !important; /* Outlook */
                }
            .center div{
                text-align: center !important;
            }
            .padding div{
                padding-left: 30px !important;
                padding-right: 50px !important;
            }
            .form td{
                padding-left: 11px !important;
                padding-right: 11px !important;
            }
            }
    //- Body
    mj-body(background-color='#ffffff' width="600px")
        //- content
        block content
        //- bloc presentation
            block offre
            include ../../dubaiThree/src/includes/_presentation
        //- bloc d√©tails
            block details
            include ../../dubaiTwo/src/includes/_two
        //-bloc footer
            block footer
            include ../../dubaiThree/src/includes/_footer